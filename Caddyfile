thedisciplineprogram.com {

    handle /api/* {
        reverse_proxy backend:8080
    }

    handle /dev-api/* {
        uri replace /dev-api/ /api/
        reverse_proxy backend-dev:8080
    }

    handle /swagger-ui/* {
        reverse_proxy localhost:8080
    }

    handle /v3/api-docs {
        reverse_proxy localhost:8080
    }
}